<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping 	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping package="com.neodem.madness.db.beans"><class name="Team" table="TournamentTeam">		<id name="id" column="tournamentTeamID" type="long" unsaved-value="0">		<generator class="native"/>	</id>		<property name="seed" column="seed" type="int" not-null="true"/>	<many-to-one name="teamInfo" class="TeamInfo" column="teamID" cascade="save-update" unique="true"/>	<many-to-one name="conference" class="Conference" column="conference" cascade="save-update" unique="true"/>    			</class><query name="QueryGetAllInConference"><![CDATA[FROM Team t WHERE t.conference = :conferenceID]]></query></hibernate-mapping>