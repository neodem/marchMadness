<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping 	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping package="com.neodem.madness.db.beans"><class name="TeamInfo" table="Team">		<id name="id" column="teamID" type="long">		<generator class="native"/>	</id>		<property name="name" column="name" type="string" not-null="true"/>	<property name="teamName" column="teamName" type="string"/>	<property name="school" column="school" type="string"/>	<property name="coach" column="coach" type="string"/>		<set name="attributes" table="TeamAttribute" lazy="false">		<key column="teamID"/>		<composite-element class="TeamAttribute">			<parent name="team"/>			<many-to-one name="attribute" class="Attribute" column="attributeID" not-null="true"/>			<property name="value" column="value" type="int"/>		</composite-element>	</set>		</class></hibernate-mapping>